body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
input,
textarea,
p,
blockquote,
th,
td {
    padding: 0;
    margin: 0;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

fieldset,
img {
    border: 0;
}

address,
caption,
cite,
code,
dfn,
em,
strong,
th,
var {
    font-weight: normal;
    font-style: normal;
}

ol,
ul {
    list-style: none;
}

caption,
th {
    text-align: left;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-weight: normal;
    font-size: 100%;
}

q:before,
q:after {
    content: '';
}

abbr,
acronym {
    border: 0;
}

body {
    font-size: 14px;
}

.clearfix:before,
.clearfix:after {
    content: "";
    display: table;
    clear: both;
    *zoom: 1;
    /*IE/7/6*/
}

.clearfix,
.list-div {
    clear: both;
    zoom: 1
}

a {
    text-decoration: none;
}

a:focus {
    outline: none;
}


/*menu部分样式*/

.header {
    background: #237ae4;
    height: 50px;
}

.header .log {
    float: left;
    height: 40px;
    margin-left: 30px;
    margin-right: 50px;
    margin-top: 5px;
}

.header ul {
    height: 50px;
    border-width: 0 0 0 0;
    padding: 0 0 0 0;
}

.main-menu-links {
    float: left;
}

.header li {
    float: left;
    padding-left: 25px;
}
.main-menu-links li{
    margin-left: 30px;
}
.main-menu-links a {
    background-color: transparent;
    border-width: 0 0 0 0;
    line-height: 50px;
    height: 50px;
    color: #fff;
}

.main-menu-transcripts {
    background: transparent url("../img/cj_icon.png") no-repeat scroll 0 18px;
}

.main-menu-messages {
    background: transparent url("../img/massage_icon.png") no-repeat scroll 0 18px;
    padding-left: 20px;
}

.main-menu-setting {
    background: transparent url("../img/sz_icon.png") no-repeat scroll 0 18px;
    padding-left: 20px;
}

.main-menu-right {
    float: right;
    margin-right: 20px;
}

.main-menu-right li {
    padding: 0;
}

.main-menu-right a {
    color: #fff;
    padding: 5px 15px;
    margin-top: 10px;
    display: block;
}

.main-menu-right .name a {
    background: none;
}
.main-content {
    float: left;
    margin-left: 0;
    margin-right: -100%;
    padding: 0;
    width: 100%;
}

.region {
    float: left;
    margin-left: 0;
    margin-right: -200px;
    padding: 0;
    width: 200px;
    z-index: 10;
}

.section-wrapper {
    background: #fff url("../img/sidebar_bg.png") repeat-y scroll left top;
}

.main-content .section {
    min-height: 450px;
    padding-left: 240px;
    padding-right: 20px;
}
.region .menu {
    margin: 15px 0;
}

.region .menu li {
    height: 44px;
    line-height: 44px;
}
.region .menu .menu2-title {
    color: #999;
    padding-left: 50px;
    background-position: 30px 15px;
}
.region .menu a {
    color: #666;
    padding-left: 50px;
    display: block;
}

.menu2-user-manage {
    background: url("../img/icon_system.png") no-repeat;
}

.menu2-transcripts-manage {
    background: url("../img/icon_education.png") no-repeat;
}

.region .menu li a:hover {
    background: #237ae4;
    color: #fff;
}


/*menu部分样式end*/

.page_title {
    background: url("../img/star.png") no-repeat scroll left top;
    padding-bottom: 30px;
    padding-left: 40px;
    margin: 30px 0;
    font-size: 24px;
    font-weight: normal;
    line-height: 1;
    border-bottom: 1px solid #ccc;

}

.title_two {
    line-height: 28px;
    margin: 10px 0;
    padding-bottom: 10px;
    vertical-align: middle;
}

.title_two .name {
    font-size: 16px;
    vertical-align: middle;
}

.publish_icon {
    display: inline-block;
    height: 13px;
    line-height: 13px;
}

.publish_icon>img {
    height: 18px;
    vertical-align: middle;
}

.publish_icon>span {
    display: none;
}

.publish_icon:hover span {
    display: inline;
}

.tabs>li {
    border: 1px solid #E1E1E1;
    border-bottom: 1px solid #237ae4;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    float: left;
    line-height: 20px;
    margin-bottom: -1px;
    margin-left: 5px;
    padding: 5px 8px;
}

.tabs>li.active {
    background: #fff;
    border: 1px solid #237ae4;
    border-bottom: 1px solid #fff;
    margin-bottom: -1px;
}

.tabs {
    border-bottom: 1px solid #237ae4;
    margin-bottom: -1px;
}

.tabs li>a {
    color: #333;
}

.child_tabs {
    margin: 15px 0;
}

.child_tabs li {
    float: left;
    margin-right: 7px;
}

.child_tabs li>a {
    color: #666;
    padding: 5px 10px;
}



.page1_table {
    width: 100%;
    margin: 20px 0 40px;
    font-size: 12px;
}

.page1_table th {
    color: #fff;
    background: #7bafef;
}

.page1_table td,
.page1_table th {
    /*border-bottom: 1px solid #f2f2f2;*/
    padding: 4px 9px;
    text-align: left;
}

.page1_table tr a {
    color: #237ae4;
}
.page1_table tr a:hover{
    text-decoration: underline;
}
table .odd {

    background: #f2f2f2;

}

table .even {
    background: #fff;
}

table .odd td,
.page1_table th {
    border-right: 1px solid #fff;
    border-bottom: 1px solid #fff;
}


/*翻页*/

.page_turn li a {
    background: #ccc none repeat scroll 0 0;
    color: #fff;
    display: block;
    margin: 0 2px;
    padding: 2px 8px;
}

.page_turn {
    text-align: center;
    margin: 10px auto;
}

.page_turn li {
    display: inline-block;
    *float: left;
}

.page_turn .active a {
    background: #0071b3;
}


/*搜索*/

.search_inline input {
    margin: 10px 8px 10px 0;
}

.search_inline .blue_button,
.gray_button {
    background: #6A9EE4;
    color: #fff;
    border: 0 none;
    padding: 3px 8px;
}

.form_text {
    width: 200px;
    padding: 4px;
    border: 1px solid #ccc;
}

.form_submit {
    border-radius: 15px;
    border: 1px solid #ccc;
    padding: 5px 8px;
    background: #F2F2F2;
    color: #666666;
    cursor: pointer;
}

.gray_button {
    background: #F2F2F2;
    border-radius: 5px;
    color: #000;
}

.gray_button.active {
    background: #6A9EE4;
    color: #fff;
}

.right {
    float: right;
}

.third {
    display: none;
}

.backToTop {
    bottom: 10px;
    position: fixed;
    right: 0;
}

.border_blue_btn,.child_tabs .active,.down_btnbox a{
    border: 1px solid #237ae4;
    color: #237ae4;
    -khtml-border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    background: #FFFFFF;
    cursor: pointer;
}
.border_blue_btn{
    padding: 5px;
}
.border_blue_btn:hover,.child_tabs .active:hover,.down_btnbox a:hover{
    background: #237ae4;
    color: #FFFFFF;
    text-decoration: none;
}

/**圆角蓝色空按钮*/
.border_blue_btn_round{
    width: 60px;
    height: 30px;
    border: 1px solid #237ae4;
    color: #237ae4;
    background: #FFFFFF;
    border-radius: 15px;
    padding: 5px;

}
.border_blue_btn_round:hover{
    background: #237ae4;
    color: #FFFFFF;
}
.border_red_btn{
    background: #FFFFFF;
    color: #e52222;
    padding: 5px;
    border: 1px solid #e52222;
    border-radius: 4px;
}
.border_red_btn:hover{
    background: #e52222;
    color: #FFFFFF;
    text-decoration: none;
    cursor: pointer;
}
.child_tabs .active:hover a{
    color: #fff;
}
.child_tabs li::before {
    color: #666;
    content: "|";
    height: 10px;
    width: 1px;
}
.child_tabs li.first::before {
    content: "";
}
.mar_10{
    margin: 10px 0;
}
.selectbox{
    margin: 15px 0;
}
.selectbox select{
    height: 30px;
    line-height: 30px;
    border:  1px solid #ccc;
    border-radius: 4px;
}
.down_btnbox{
    float: right;
    margin: 10px 0 30px;
}
.down_btnbox a{
    padding: 4px 6px;
    margin-left: 10px;
    font-size: 12px;
}
