    border: 1px solid #e1e1e1;
    border-radius: 4px;
    margin-bottom: 20px;
    padding-bottom: 15px;
}
.clearfix, .list-div {
    clear: both;
}
.chartbox {
    border: 1px solid #f2f2f2;
    border-radius: 5px;
}
.chartbox .title {
    background: #f2f2f2 none repeat scroll 0 0;
    color: #666;
    height: 30px;
    line-height: 30px;
    padding-left: 10px;
}
.chart-contenter {
    padding: 30px 15px;
}
.uploadimg-box {
    display: block;
    margin-top: 20px;
    position: relative;
    text-align: center;
    width: 100px;
}
.blue-btn {
    border-radius: 4px;
    padding: 5px 14px;

}
.sgm-blue-btn{
    margin-top: 10px;
    display: inline-block;
}
.border_blue_btn:hover, .child_tabs .active:hover, .down_btnbox a:hover, .blue-btn {
    background: #237ae4 none repeat scroll 0 0;
    color: #ffffff;
}
.blue-btn > input[type="file"] {
    cursor: pointer;
    left: 0;
    opacity: 0;
    position: absolute;
    top: 0;
    width: 100%;
}

