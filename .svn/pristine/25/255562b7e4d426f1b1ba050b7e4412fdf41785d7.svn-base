input.form-submit, a.button {
    background: #fff url(../../img/buttons.png) 0 0 repeat-x;
    border: 1px solid #e4e4e4;
    border-bottom: 1px solid #b4b4b4;
    border-left-color: #d2d2d2;
    border-right-color: #d2d2d2;
    color: #3a3a3a;
    cursor: pointer;
    font-weight: normal;
    text-align: center;
    margin-top: 0.5em;
    margin-bottom: 1em;
    margin-right: 0.6em;
    padding: 4px 17px;
    -khtml-border-radius: 15px;
    -moz-border-radius: 20px;
    -webkit-border-radius: 15px;
    border-radius: 15px;
}
input.form-submit:hover, input.form-submit:focus {
    background: #dedede;
}
input.form-submit.red-button, a.red-button {
    background: rgb(255,45,3) none;
    border: none;
    border-radius: 0;
    color: #fff;
    padding: 4px 17px;
}
input.form-submit.red-button:hover, input.form-submit.red-button:focus, a.red-button:hover, a.red-button:focus {
    color: #fff;
    text-decoration: none;
    background: rgb(255,45,3) none;
}
table tr th, table tr th a, table tr th a:hover {
    color: #FFF;
    font-weight: bold;
}
table tr th {
    background: #7bafef;
    border-bottom-style: none;
}

.triangle-item {
    background: url(../../img/round-bg.png) no-repeat;
    width: 27px;
    height: 30px;
    color: #fff;
    text-indent: 10px;
    padding-top: 3px;
    display: inline-block;
}

.form-item {
    margin-bottom: 1em;
    margin-top: 2px;
    display: block;
}
.form-item label {
    display: inline-block;
    font-size: 0.929em;
    font-weight: bold;
    margin-right: 6px;
    width: 70px;
}

.form-item .description {
    font-size: 0.85em;
}
textarea, select.form-select {
    border: 1px solid #ccc;
}
div.password-strength, div.password-suggestions {
    display: none !important;
}
.password-strength {
    width: 17em;
    float: right;
    margin-top: 1.4em;
}
div.password-confirm {
    float: right;
    margin-top: 1.5em;
    visibility: hidden;
    width: 17em;
}
.password-strength-text, .password-strength-title, div.password-confirm {
    font-size: 0.82em;
}
.form-required {
    color: #c00;
}
.item-indent {
    margin-left: 30px;
}

.form-item label.option {
    display: inline;
    font-weight: normal;
}
.form-type-radio label, .form-type-checkbox label {
    margin-left: 4px;
}
body, body.overlay {
    color: #3b3b3b;
}
a.button:link, a.button:visited, a.button:hover, a.button:focus, a.button:active {
    text-decoration: none;
    color: #5a5a5a;
}
p {
    margin: 0 0 1.2em;
}
.container-inline div, .container-inline label {
    display: inline;
}

.personSet-section{
    padding-left: 20px;
    width: 100%;
}


.clearfix::before, .clearfix::after {
    clear: both;
    content: "";
    display: table;
}
