* {
    margin: 0px;
    padding: 0px;
}

body {
    line-height: 1.5;
}

.div_top {
    width: 100%;
    height: 30px;
    background-image: url(../image/nav_bg.jpg);
}

.div_top_content {
    width: 1000px;
    margin: 0 auto;
}

.div_top_content li {
    float: left;
    list-style: none;
    list-style-image: url(../image/arrow.jpg);
    width: 80px;
    line-height: 30px;
}

.div_top_content a:link, .div_top_content a:visited {
    color: #8e8e8e;
    text-decoration: none;
}

.div_top_content a:hover, .div_top_content a:active {
    color: crimson;
    text-decoration: none;
}

.divbody {
    width: auto;
    height: 1000px;
    background-color: cadetblue;
}

.div_left_bar {
    width: 200px;
    height: 1000px;
    background-color: gray;
}

.div_right_content {
    width: 500px;
    height: 1000px;
    background-color: indianred;
}

.div_content {
    width: 1200px;
    height: auto;
    margin-top: 10px;
}

table {

    font-size: 0.857em;
    margin: 10px 0;
    border-collapse: collapse;
    border: 0;
    border-spacing: 0;
    width: 100%;
}

table.system_table td {
    padding-top: 0.5em;
    padding-bottom: 0.5em;
    border-bottom: #ccc 1px solid;
}

tr td, tr th {
    padding: 4px 9px;
    border: 1px solid #fff;
    text-align: left;
}

.bigger {
    font-size: 1.5em;
}

img {
    vertical-align: middle;
}

.blue-round-box {
    background-color: rgb(106, 158, 228);
    color: #fff;
    display: inline-block;
    padding: 0.9em 0.9em;
    border-radius: 10px;
}

.center {
    text-align: center;
}

a {
    color: #0071B3;
}

a:hover, a:focus {
    color: #018fe2;
}

a:hover, a:active, a:focus {
    text-decoration: underline;
}
.div_content_student{
    margin-top: 20px;
}
